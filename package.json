{
  "name": "edukat",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "next build && tsc --project tsconfig.server.json",
    "knex": "ts-node --project server/tsconfig.json --require dotenv/config node_modules/knex/bin/cli.js --knexfile server/config/database.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "start": "node dist/index.js",
    "start:dev": "nodemon",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:dev": "jest --watch"
  },
  "dependencies": {
    "argon2": "^0.27.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "knex": "^0.21.5",
    "luxon": "^1.25.0",
    "next": "^9.5.2",
    "pg": "^8.3.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.10",
    "@types/luxon": "^1.24.3",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.46",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-import-resolver-typescript": "^2.2.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.0.8",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.2.0",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7"
  }
}
