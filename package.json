{
  "name": "edukat",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build:server": "tsc --project server/tsconfig.json",
    "build": "next build && yarn build:server",
    "knex": "ts-node --project server/tsconfig.json --require tsconfig-paths/register --require dotenv/config node_modules/knex/bin/cli.js --knexfile server/config/database.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "setup": "yarn knex migrate:latest && yarn knex seed:run",
    "setup:rollback": "yarn knex migrate:rollback --all",
    "start": "node dist/index.js",
    "start:dev": "nodemon",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:dev": "jest --watch",
    "typecheck": "yarn build:server --noEmit"
  },
  "dependencies": {
    "argon2": "^0.27.0",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.1",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "^0.12.2",
    "express": "^4.17.1",
    "handlebars": "^4.7.6",
    "ioredis": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.5",
    "luxon": "^1.25.0",
    "next": "^9.5.2",
    "nodemailer": "^6.4.11",
    "pg": "^8.3.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.3.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@types/express": "^4.17.7",
    "@types/faker": "^4.1.12",
    "@types/ioredis": "^4.17.3",
    "@types/jest": "^26.0.10",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/luxon": "^1.24.3",
    "@types/node": "^14.0.27",
    "@types/nodemailer": "^6.4.0",
    "@types/react": "^16.9.46",
    "@types/rosie": "^0.0.37",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "commitizen": "^4.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-import-resolver-typescript": "^2.2.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.0.8",
    "faker": "^5.1.0",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "jest-chain": "^1.1.5",
    "jest-extended": "^0.11.5",
    "lint-staged": "^10.3.0",
    "nodemon": "^2.0.4",
    "rosie": "^2.0.1",
    "ts-jest": "^26.2.0",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
